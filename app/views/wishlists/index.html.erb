<h2 class="page-titles">Listing wishlists</h2>

<table>
  <tr>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @wishlists.each do |wishlist| %>
  <tr>
    <td><%= link_to 'Show', wishlist %></td>
    <td><%= link_to 'Edit', edit_wishlist_path(wishlist) %></td>
    <td><%= button_to 'Destroy', wishlist, :confirm => 'Are you sure?', :method => :delete, :class => 'destroy-btns' %></td>
  </tr>
<% end %>
</table>


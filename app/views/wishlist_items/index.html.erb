<h1>Listing wishlist_items</h1>

<table>
  <tr>
    <th>Product</th>
    <th>Wishlist</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @wishlist_items.each do |wishlist_item| %>
  <tr>
    <td><%= wishlist_item.product_id %></td>
    <td><%= wishlist_item.wishlist_id %></td>
    <td><%= link_to 'Show', wishlist_item %></td>
    <td><%= link_to 'Edit', edit_wishlist_item_path(wishlist_item) %></td>
    <td><%= link_to 'Destroy', wishlist_item, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Wishlist item', new_wishlist_item_path %>
